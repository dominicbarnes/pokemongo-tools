    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "sift": "^7.0.1",
    "sort-by": "^1.2.0",
    "vue": "^2.6.10",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.6",
    "vue-slider-component": "^2.8.16",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "wait-port": "^0.2.2"
  },
  "scripts": {
    "couchdb": "docker-compose up -d && wait-port localhost:5984 && couchdb-bootstrap http://admin:secret@localhost:5984 _couchdb",
    "prebuild": "mkdir -p public",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "predev": "npm run couchdb",
    "dev": "concurrently 'npm start' 'npm run watch'",
    "start": "hoodie --port $PORT --in-memory",
    "stop": "docker-compose stop",
    "clean": "git clean -fdx ./public",
    "distclean": "docker-compose down",
    "test": "eslint .",
    "preimport": "docker-compose up -d",
    "import": "node scripts/import.js",
    "mine": "node scripts/mine.js",
    "version": "git-changelog --tag $npm_package_version && git add HISTORY.md",
    "postversion": "git push --tags && git push",
    "precommit": "npm test",
    "update": "git submodule update --recursive --remote"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "standard",
      "plugin:vue/essential"
    ],
    "rules": {
      "node/no-deprecated-api": 0
    }
  },
  "eslintIgnore": [
    "_couchdb/**",
    "public/**",
    "pokemongo-game-master/**",
    "POGOProtos/**"
  ]
}
