    "empty-element": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "fs-extra": "^7.0.1",
    "husky": "^0.14.3",
    "json-stable-stringify": "^1.0.1",
    "moment": "^2.24.0",
    "nano": "^6.4.4",
    "nano-blue": "^0.3.0",
    "nano-markdown": "^1.2.0",
